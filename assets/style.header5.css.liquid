  align-items: center;
  line-height: 13px;
  margin-left: 5px;
  font-weight: 300;
}
.header .cart-block .total-price span:last-child {
  margin-left: 2px;
}
.header .header__counter {
    position: absolute;
    top: -10px;
    left: 20px;
