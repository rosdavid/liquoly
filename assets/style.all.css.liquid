	 color: inherit;
	 opacity: 1;
	 filter: alpha(opacity=100);
}
 :-moz-placeholder {
	 color: inherit;
	 opacity: 1;
	 filter: alpha(opacity=100);
}
 ::-moz-placeholder {
	 color: inherit;
	 opacity: 1;
	 filter: alpha(opacity=100);
