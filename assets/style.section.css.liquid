  font-size: 16px;
  line-height: 24px;
  color: #222;
}
.specs-main{
  padding: 40px;
  background-color: #f5f8fd;
  font-size: 16px;
  line-height: 24px;
  text-align:left;
  border-radius: 10px;
}
.specs-main .spec-items ul {
  list-style: none;
  padding: 15px 0 0;
  margin: 0;
}
.specs-main .spec-items ul li{
  padding: 12px 0;
}
i.icon.icon-alert-circle{
  margin-right: 10px;
}
i.icon.icon-file-text{
  margin-right: 10px;
}
i.icon.icon-star_outline{
  margin-right: 10px;
}
i.icon.icon-users{
  margin-right: 10px;
}
figure.image{
  display: table;
  margin-left: auto;
  margin-right: auto;
}
.button-productor{
  background-color:#1e34af;
	border-radius:15px;
	display:inline-block;
	cursor:pointer;
	color:#ffffff;
	font-family:Arial;
	font-size:17px;
	padding:16px 31px;
	text-decoration:none;
}
.button-productor:hover{
  background-color: #6a99f7;
  color:#ffffff;
}