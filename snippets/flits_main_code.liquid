{% assign flitsBirthdate =  customer.metafields.Flits.birthdate | split: '-'  %}
{% assign flitsBirthdateDay =  flitsBirthdate[2] | plus:0  %}
{% assign flitsBirthdateMonth =  flitsBirthdate[1] | plus:0  %}
{% assign flitsBirthdateYear =  flitsBirthdate[0] | plus:0  %}

{%- case flits_main_code -%}

{%- when 'module_profile' -%}
	<div class="flits-account-box flits-with-boundry flits-profile-div" id="{{ id }}"> </div>
{%- when 'module_profile_content' -%}
    <div class="flits-container-box flits-profile-form-container">
		<form action="/profile_save" class="flits-profile-form flits-read-input" method="post">
			<div class="flits-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_firstNameText,
						placeholder:profile_firstNameText,
						value:customer.first_name,
						name: 'first_name',
						attr:'disabled'
					%} 
				</div>
				<div class="flits-col-md-6  flits-col-sm-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_lastNameText, 
						placeholder:profile_lastNameText,
						value:customer.last_name,
						name: 'last_name',
						attr:'disabled'
					%} 
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'email', 
						label:profile_emailText, 
						placeholder:profile_emailText,
						value:customer.email,
						name: 'email',
						attr:'disabled'
					%}
					<small class="flits-profile-email-alert flits-form-alert"></small>
				</div>
				<div class="flits-col-md-6  flits-col-sm-6 flits-mb-15">
					<div class="flits-phone-with-country-div flits-profile-phone-with-country-div flits-readonly-mode">
						<div class="flits-input-wrap">
							<label class="flits-input-label">{{ profile_contactNoText | strip }}</label>
							<input type="tel" class="flits-input" name="phone" value="{{ customer.phone }}" placeholder="{{ profile_contactNoText | strip }}" disabled>
							<input type="text" class="flits-input flits-country-code-textbox" value="" disabled>
							<div class="flits-country-drpdown">
								<span class="flits-country-flag"></span>
								<div class="flits-select-row">
									<select class="flits-input"></select>
									<div class="flits-select-arrow">
										<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
											<path d="M6 9l6 6 6-6"></path>
										</svg> 
									</div>
								</div>
							</div>
						</div>
					</div>
					<small class="flits-profile-phone-alert flits-form-alert"></small>
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-mb-15">
					<div class="flits-input-wrap">
						<label class="flits-input-label">{{ profile_birthdateText | strip }}</label>
						<div class="flits-dob-wrap" data-flits-birthdate="{{ customer.metafields.Flits.birthdate }}">
							<div class="flits-select-row flits-dd-select">
								<select class="flits-input"  disabled name="birthday">
									<option value="dd" disabled {% if flitsBirthdateDay == 0 %}selected{% endif %}>DD</option>
								</select>
								<div class="flits-select-arrow">
									<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
										<path d="M6 9l6 6 6-6"></path>
									</svg> 
								</div>
							</div>
							<div class="flits-select-row flits-mm-select flits-mx-10">
								<select class="flits-input"  disabled name="birthmonth">
									<option value="mm" disabled {% if flitsBirthdateMonth == 0 %}selected{% endif %}>MM</option>
								</select>
								<div class="flits-select-arrow">
									<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
										<path d="M6 9l6 6 6-6"></path>
									</svg> 
								</div>
							</div>
							<div class="flits-select-row flits-yy-select">
								<select class="flits-input"  disabled name="birthyear">
									<option value="yyyy" disabled {% if flitsBirthdateYear == 0 %}selected{% endif %}>YYYY</option>
								</select>
								<div class="flits-select-arrow">
									<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
										<path d="M6 9l6 6 6-6"></path>
									</svg> 
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="flits-row flits-gender-radio-row">
				<div class="flits-col-md-6 flits-col-sm-6">
					<div class="flits-input-wrap flits-gender-input-wrap">
						<label class="flits-input-label">{{ profile_genderText | strip }}</label>
						<div class="flits-gender-input-row flits-gender-radio">
							<div class="flits-input-row">
								<input type="radio" class="flits-input" id="flits-gender-male" 
									name="gender" value="male" disabled {% if customer.metafields.Flits.gender == 'male' %}checked{% endif %}>
								<label class="flits-input-label" for="flits-gender-male">{{ profile_maleText | strip }}</label>
							</div>
							<div class="flits-input-row">
								<input type="radio" class="flits-input" id="flits-gender-female" 
									name="gender" value="female" disabled {% if customer.metafields.Flits.gender == 'female' %}checked{% endif %}>
								<label class="flits-input-label" for="flits-gender-female">{{ profile_femaleText | strip }}</label>
							</div>
							<div class="flits-input-row">
								<input type="radio" class="flits-input" id="flits-gender-other" 
									name="gender" value="other" disabled {% if customer.metafields.Flits.gender == 'other' %}checked{% endif %}>
								<label class="flits-input-label" for="flits-gender-other">{{ profile_otherText | strip }}</label>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="flits-row flits-gender-select-row">
				<div class="flits-col-md-6 flits-col-sm-6">
					<div class="flits-input-wrap">
					<label class="flits-input-label">{{ profile_genderText | strip }}</label>
						<div class="flits-select-row">
							<select class="flits-input flits-gender-select" disabled data-flits-gender="{{ customer.metafields.Flits.gender }}">
								{% unless customer.metafields.Flits.gender %}
									<option value="none" class="flits-hide"></option>
								{% endunless %}
								<option value="male" {% if customer.metafields.Flits.gender == 'male' %}selected{% endif %}>{{ profile_maleText | strip }}</option>
								<option value="female" {% if customer.metafields.Flits.gender == 'female' %}selected{% endif %}>{{ profile_femaleText | strip }}</option>
								<option value="other" {% if customer.metafields.Flits.gender == 'other' %}selected{% endif %}>{{ profile_otherText | strip }}</option>
							</select>
							<div class="flits-select-arrow">
								<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
									<path d="M6 9l6 6 6-6"></path>
								</svg> 
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="flits-row flits-hide flits-referby-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-mt-15 flits-read-input">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						name: 'referby',
						label:refer_youAreReferredByText,
						attr:'disabled'
					%}
				</div>
			</div>
			<div class="flits-edit-form-btn-grp flits-hide">
				<div class="flits-row flits-text-center">
					<div class="flits-col-md-12 flits-mt-30">
						{% render 'flits_capture_code' with 'component',template:'comp_button',
							type:'button',
							text:profile_buttonSaveText,
							class:'flits-primary-btn flits-profile-save-button'
						%} 
					</div>
				</div> 
			</div>
			<div class="flits-button-float">
				<div class="flits-button-icon flits-profile-edit-button" data-tippy-content="{{ profile_buttonEditText | strip }}">
					<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 25.13 25"><path d="M25.07,15.86v5.85A3.29,3.29,0,0,1,21.78,25H3.29A3.3,3.3,0,0,1,0,21.71V4.54A3.29,3.29,0,0,1,3.29,1.25H9.14a.66.66,0,1,1,0,1.31H3.29a2,2,0,0,0-2,2V21.71a2,2,0,0,0,2,2H21.78a2,2,0,0,0,2-2V15.86a.66.66,0,0,1,1.32,0Zm-11.67-1L23,5.21l-3.1-3.1L10.3,11.73ZM8.73,16.4l3.43-.95L9.68,13ZM23.88.48a1.65,1.65,0,0,0-2.33,0l-.7.7L24,4.28l.7-.7a1.64,1.64,0,0,0,0-2.33Z"/></svg>
				</div>
				<div class="flits-button-icon flits-profile-cancel-button flits-hide" data-tippy-content="{{ profile_buttonCancelText | strip }}">
					<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 25 25"><path id="Layer_2" data-name="Layer 2" d="M12.5,0A12.5,12.5,0,1,0,25,12.5,12.5,12.5,0,0,0,12.5,0Zm0,24.2A11.7,11.7,0,1,1,24.2,12.5,11.65,11.65,0,0,1,12.5,24.2ZM18.2,6.8a.38.38,0,0,0-.6,0h0l-5.1,5.1L7.4,6.8a.37.37,0,0,0-.6,0,.38.38,0,0,0,0,.6h0l5.1,5.1L6.8,17.6a.38.38,0,0,0,0,.6.37.37,0,0,0,.6,0h0l5.1-5.1,5.1,5.1a.37.37,0,0,0,.6,0h0a.37.37,0,0,0,0-.6l-5.1-5.1,5.1-5.1a.38.38,0,0,0,0-.6Z"/></svg>
				</div>
			</div>
		</form>
	</div>

{%- when 'module_mobile_profile' -%}
	<div class="flits-mobile-account-box flits-with-boundry flits-profile-div" id="{{ id }}"> </div>
{%- when 'module_mobile_profile_content' -%}
	<div class="flits-navigation-header">
		{% render 'flits_capture_code' with 'component',template:'comp_mobile_logutButton' %}
		<div class="flits-user-avatar" data-flits-name="{{ customer.first_name | slice: 0 | strip }}{{ customer.last_name | slice: 0 | strip }}">
		</div>
		<div class="flits-user-box flits-skeleton-user-box">
			<p class="flits-h4 flits-greeting-title flits-skeleton-box flits-skeleton-greeting-mobile"></p>
			<span class="flits-user-name flits-skeleton-box flits-skeleton-user-name-mobile" data-tippy-content="{{ customer.first_name | strip }}">{{ customer.first_name | strip }}</span>
		</div>
	</div>
	<div class="flits-mobile-container-box flits-profile-form-container flits-mt-25 flits-px-15">
		<form action="/profile_save" class="flits-profile-form flits-read-input" method="post">
			<div class="flits-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-px-0">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_firstNameText,
						placeholder:profile_firstNameText, 
						value:customer.first_name,
						name: 'first_name',
						attr:'disabled'
					%} 
				</div>
				<div class="flits-col-md-6  flits-col-sm-6 flits-px-0">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_lastNameText,
						placeholder:profile_lastNameText, 
						value:customer.last_name,
						name: 'last_name',
						attr:'disabled'
					%} 
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6  flits-col-sm-6 flits-px-0">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'email', 
						label:profile_emailText,
						placeholder:profile_emailText, 
						value:customer.email,
						name: 'email',
						attr:'disabled'
					%}
					<small class="flits-profile-email-alert flits-form-alert"></small>
				</div>
				<div class="flits-col-md-6  flits-col-sm-6 flits-px-0">
					<div class="flits-phone-with-country-div flits-profile-phone-with-country-div flits-readonly-mode">
						<div class="flits-input-wrap">
							<label class="flits-input-label">{{ profile_contactNoText | strip }}</label>
