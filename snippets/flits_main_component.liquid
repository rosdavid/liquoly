      </div>
      <div class="flits-popup-text">
        <p class="flits-h3 flits-popup-header-title">{{ contactUs_formTitleText | strip }}</p>
        <p class="flits-popup-header-sub-text">{{ contactUs_headerText | strip }}</p>
      </div>
    </div>
    <span class="flits-close-popup">&times;</span>
    <div class="flits-form-content">
      <!-- Form only for display purpose -->
      {% assign formId = 'flits-contact-form' %}
      <script>
        window.flitsApp.formId = "{{ formId }}"
      </script>
      {% form 'contact', id: formId %}
      <div class="flits-row">
        <div class="flits-col-sm-6 flits-col-md-6 flits-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-first-name">{{ contactUs_FirstNameLabelText | strip }}</label>
            <div class="flits-input-icon-group">
              <div class="flits-input-icon">
                <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
              </div>
              <input type="text" class="flits-input flits-contact-form-first-name" id="{{ formId }}-first-name"
                     placeholder="{{ contactUs_firstNamePlaceholderText | strip }}"
                     name="contact[{{ contactUs_FirstNameLabelText | strip }}]"
                     value="{% if form[first_name] %}{{ form[first_name] }}{% elsif customer %}{{ customer.first_name }}{% endif %}">
            </div>
          </div>
        </div>
        <div class="flits-col-sm-6 flits-col-md-6 flits-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-last-name">{{ contactUs_lastNameLabelText | strip }}</label>
            <div class="flits-input-icon-group">
              <div class="flits-input-icon">
                <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
              </div>
              <input type="text" class="flits-input flits-contact-form-last-name" id="{{ formId }}-last-name"
                     placeholder="{{ contactUs_lastNamePlaceholderText | strip }}"
                     name="contact[{{ contactUs_lastNameLabelText | strip }}]"
                     value="{% if form[last_name] %}{{ form[last_name] }}{% elsif customer %}{{ customer.last_name }}{% endif %}">
            </div>
          </div>
        </div>
      </div>
      <div class="flits-row">
        <div class="flits-col-sm-12 flits-col-md-6 flits-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-email">{{ contactUs_emailLabelText | strip }} <span class ="flits-requird-sign" aria-hidden="true">*</span></label>
            <div class="flits-input-icon-group">
              <div class="flits-input-icon">
                <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
              </div>
              <input type="text"
                     placeholder="{{ contactUs_emailPlaceholderText | strip }}"
                     id="{{ formId }}-email"
                     name="contact[{{ contactUs_emailLabelText | strip }}]"
                     autocorrect="off"
                     autocapitalize="off"
                     value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                     aria-required="true"
                     class="flits-input flits-contact-form-email">
            </div>
          </div>
          <small id="flits-alert-email" class="flits-form-alert"></small>
        </div>
        <div class="flits-col-sm-12 flits-col-md-6 flits-mb-15">
          <div class="flits-phone-with-country-div flits-contact-us-phone-number-country-div">
            <div class="flits-input-wrap flits-input-icon-wrap">
              <label class="flits-input-label" for="{{ formId }}-phone">{{ contactUs_phoneNumberLabelText | strip }}</label>
              <div class="flits-input-icon-group">
                <div class="flits-input-icon">
                  <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg>
                </div>
                <input type="tel" class="flits-input flits-contact-form-phone" id="{{ formId }}-phone"
                       placeholder="{{ contactUs_phoneNumberPlaceholderText | strip }}"
                       name="contact[{{ contactUs_phoneNumberLabelText | strip }}]"
                       value="{% if form[phone] %}{{ form[phone] }}{% elsif customer %}{{ customer.phone }}{% endif %}" data-name="contact[phone]">
                <input type="text" class="flits-input flits-country-code-textbox" disabled>
                <div class="flits-country-drpdown">
                  <span class="flits-country-flag"></span>
                  <div class="flits-select-row">
                    <select class="flits-input"></select>
                    <div class="flits-select-arrow">
                      <svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
                        <path d="M6 9l6 6 6-6"></path>
                      </svg> 
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <small id="flits-alert-phone" class="flits-form-alert"></small>
        </div>
      </div>
      <div class="flits-row">
        <div class="flits-col-md-12 flits-mb-15">
          <div class="flits-input-wrap">
            <label class="flits-input-label" for="{{ formId }}-reason">{{ contactUs_reasonLabelText | strip }} <span class ="flits-requird-sign" aria-hidden="true">*</span></label>
            <div class="flits-select-row">
              <select class="flits-input flits-contact-form-reason" name="contact[{{ contactUs_reasonLabelText | strip }}]" id="{{ formId }}-reason">
                <option value="" disabled="" selected="">{{ contactUs_selectAnyReasonText | strip }}</option>
                <option value="{{ contactUs_orderCancelRelatedReasonText | strip }}">{{ contactUs_orderCancelRelatedReasonText | strip }}</option>
                <option value="{{ contactUs_orderAndShippingRelatedReasonText | strip }}">{{ contactUs_orderAndShippingRelatedReasonText | strip }}</option>
                <option value="{{ contactUs_returnAndExchangeRelatedReasonText | strip }}">{{ contactUs_returnAndExchangeRelatedReasonText | strip }}</option>
                <option value="{{ contactUs_productRelatedReasonText | strip }}">{{ contactUs_productRelatedReasonText | strip }}</option>
                <option value="{{ contactUs_priceRelatedReasonText | strip }}">{{ contactUs_priceRelatedReasonText | strip }}</option>
                <option value="{{ contactUs_otherReasonText | strip }}">{{ contactUs_otherReasonText | strip }}</option>
              </select>
              <div class="flits-select-arrow">
                <svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
                  <path d="M6 9l6 6 6-6"></path>
                </svg> 
              </div>
            </div>
          </div>
          <small id="flits-alert-reason" class="flits-form-alert"></small>
        </div>
      </div>
      <div class="flits-row">
        <div class="flits-col-md-12 flits-mb-15">
          <div class="flits-input-wrap">
            <label class="flits-input-label" for="{{ formId }}-message">{{ contactUs_messageLabelText | strip }} <span class="flits-requird-sign" aria-hidden="true">*</span></label>
            <textarea rows="5" maxlength="250" class="flits-contact-form-message flits-textarea" id="{{ formId }}-message"
                      placeholder="{{ contactUs_messagePlaceholderText | strip }}"
                      name="contact[{{ contactUs_messageLabelText | strip }}]"></textarea>
            <span id="flits-msg-count" class="flits-text-count">0 / 250</span>
          </div>
          <small id="flits-alert-message" class="flits-form-alert"></small>
        </div>
      </div>
      <div class="flits-row">
        <div class="flits-col-sm-12 flits-col-md-12 flits-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-url">{{ contactUs_attachmentLinkLabelText | strip }}</label>
            <div class="flits-input-icon-group">
              <div class="flits-input-icon">
                <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M10 13a5 5 0 0 0 7.54.54l3-3a5 5 0 0 0-7.07-7.07l-1.72 1.71"></path><path d="M14 11a5 5 0 0 0-7.54-.54l-3 3a5 5 0 0 0 7.07 7.07l1.71-1.71"></path></svg>
              </div>
              <input type="text" class="flits-input flits-contact-form-url" id="{{ formId }}-url"
                     placeholder="{{ contactUs_attachmentLinkPlaceholderText | strip }}"
                     name="contact[{{ contactUs_attachmentLinkLabelText | strip }}]"
                     value="{% if form[url] %}{{ form[url] }}{% endif %}">
            </div>
          </div>
          <small id="flits-alert-url" class="flits-form-alert"></small>
        </div>
      </div>
      {% comment %} <p class="flits-model-footer-text"></p> {% endcomment %}
      <button class="flits-input" id="flits-contactus-submit" type="button">
        {{ contactUs_submitButtonText | strip }}
      </button>
      {% endform %}
      <!-- Actual form which is used for submit -->
      {% assign flitsFormId = 'contact-form' %}
      <script>
        window.flitsApp.flitsFormId = "{{ flitsFormId }}"
      </script>
      {% form 'contact', id: flitsFormId %}
      <!-- Mail content headline -->
      <input type="hidden" name="contact[Message received from Contact Us form of Customer Account Page]"/>

      <!-- Reason To Contact -->
      <input type="hidden" id="{{ flitsFormId }}-customer-reason" name="contact[{{ contactUs_reasonLabelText | strip }}]" value=""/>

      <!-- Customer Name -->
      <input type="hidden" id="{{ flitsFormId }}-customer-name" name="contact[{{ contactUs_customerNameLabelText | strip }}]" value=""/>

      <!-- Customer Link hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-customer-link" name="contact[{{ contactUs_customerLinkLabelText | strip }}]" value=""/>

      <!-- Email -->
      <input type="hidden" id="{{ flitsFormId }}-customer-email" name="contact[email]" value=""/>

      <!-- Phone Number -->
      <input type="hidden" id="{{ flitsFormId }}-customer-phone" name="contact[{{ contactUs_phoneNumberLabelText | strip }}]" value=""/>

      <!-- Order Link hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-link" name="contact[{{ contactUs_orderLinkLabelText | strip }}]" value=""/>

      <!-- Order Date hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-date" name="contact[{{ contactUs_orderDateLabelText | strip }}]" value=""/>

      <!-- Order Number hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-number" name="contact[{{ contactUs_orderNumberLabelText | strip }}]" value=""/>

      <!-- Order Name hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-name" name="contact[{{ contactUs_orderNameLabelText | strip }}]" value=""/>

      <!-- Order Total Price hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-total" name="contact[{{ contactUs_orderTotalPriceLabelText | strip }}]" value=""/>

      <!-- Order Payment Status hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-payment-status" name="contact[{{ contactUs_orderPaymentStatusLabelText | strip }}]" value=""/>

      <!-- Order Fulfillment Status hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-fulfillment-status" name="contact[{{ contactUs_orderFulfillmentStatusLabelText | strip }}]" value=""/>

      <!-- Message -->
      <input type="hidden" id="{{ flitsFormId }}-customer-message" name="contact[{{ contactUs_messageLabelText | strip }}]" value=""/>

      <!-- Attachment Link -->
      <input type="hidden" id="{{ flitsFormId }}-customer-url" name="contact[{{ contactUs_attachmentLinkLabelText | strip }}]" value=""/>
      {% endform %}
    </div>
  </div>
</div>
