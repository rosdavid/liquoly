{% assign modules = 'module_profile,module_address,module_order,module_topOrder,module_wishlist,module_recentlyView,module_storeCredit,module_howToEarn,module_howToSpend,module_fromAdmin,module_referFriend,module_changePassword' | split: ',' %}

{% case flits_main_component %}

{%- when 'comp_greeting' -%}
<p class="flits-h1 flits-greeting-text">
  <span class="flits-greeting-title"></span> <span class="flits-user-name">{{ customer.first_name | strip }}</span>
</p>

{%- when 'comp_menuNav' -%}
<div class="flits-menu-nav flits-skeleton-menu-nav">
  <div class="flits-navigation-header flits-with-shadow flits-skeleton-navigation-header">
    <div class="flits-user-avatar flits-skeleton-user-avatar flits-skeleton-box" data-flits-name="{{ customer.first_name | slice: 0 | strip }}{{ customer.last_name | slice: 0 | strip }}">
    </div>
    <div class="flits-user-box flits-skeleton-user-box">
      <div class="flits-d-flex flits-skeleton-box flits-skeleton-user-name">
        <p class="flits-h4 flits-user-name" data-tippy-content="{{ customer.name | strip }}">{{ customer.name | strip }}</p>
      </div>
      <div class="flits-d-flex flits-align-items-center flits-skeleton-box flits-skeleton-time">
        <svg class="flits-svg-clock">
          <g>
            <circle class="flits-clock-circle" cx="50%" cy="50%" r="45%"></circle>
          </g>
          <g class="flits-clock-hande">
            <line x1="50%" y1="50%" x2="50%" y2="25%" id="flits-hourhand">
            </line>
            <line x1="50%" y1="50%" x2="50%" y2="15%" id="flits-minutehand">
            </line>
            <line x1="50%" y1="50%" x2="50%" y2="12%" id="flits-secondhand">
            </line>
          </g>
          <circle class="flits-clock-hand-center" cx="50%" cy="50%" r="3%"></circle>
        </svg>
        <p class="flits-current-time"></p>
      </div>
    </div>
  </div>
  <div class="flits-menus-list-box flits-hide">
    <div class="flits-menu-items"></div>
  </div>
  {% render 'flits_capture_code' with 'component',template:'comp_skeletonMenu' %}
</div>

{%- when 'comp_menuItem' -%}
<div class="flits-menu-item {{class}}">
  <a href="{{- href -}}">
    <span class="flits-menu-img">{{ img  | strip }}</span>
    <span {{ attr }} class="flist-menu-title">{{- text | strip -}}</span>
  </a>
  {{ badge }}
</div>

{%- when 'comp_accountBody'-%}
<div class="flits-account-body flits-skeleton-account-body">
  {% liquid 
for module in modules
assign module_id = module | replace: 'module_', 'flits_tab_'
render 'flits_capture_code' with 'module',template:module, id:module_id
endfor 
%}
</div>

{%- when 'comp_mobile_menuNav' -%}
<div class="flits-slider-menu-nav flits-skeleton-slider-menu-nav">
  <div class="flits-menus-list-box flits-hide">
    <div class="flits-menu-items"></div>
  </div>
  {% render 'flits_capture_code' with 'component',template:'comp_mobile_skeletonMenu' %}
</div>

{%- when 'comp_mobile_accountBody'-%}
<div class="flits-mobile-account-body flits-skeleton-mobile-account-body">
  {% liquid 
for module in modules
assign module_id = module | replace: 'module_', 'flits_mobile_tab_'
assign module_name = module | replace: 'module_', 'module_mobile_'
render 'flits_capture_code' with 'module',template:module_name, id:module_id
endfor 
%}
</div>

{%- when 'comp_accountBody_card' -%}
<div class="flits-account-box {{ class }}">
  <div class="flits-account-box-header">
    <div class="flits-header-title"></div>
  </div>
  <div class="flits-container-box">
    <div class="flits-box-card"></div>
  </div>
</div>

{%- when 'comp_mobile_accountBody_card' -%}
<div class="flits-mobile-account-box {{ class }}">
  <div class="flits-navigation-header flits-with-boxshadow flits-pb-30">
    {% render 'flits_capture_code' with 'component',template:'comp_mobile_logutButton' %}
    <div class="flits-user-avatar" data-flits-name="{{ customer.first_name | slice: 0 | strip }}{{ customer.last_name | slice: 0 | strip }}">
    </div>
    <div class="flits-user-box flits-skeleton-user-box">
      <p class="flits-h4 flits-greeting-title flits-skeleton-box flits-skeleton-greeting-mobile"></p>
      <span class="flits-user-name flits-skeleton-box flits-skeleton-user-name-mobile" data-tippy-content="{{ customer.first_name | strip }}">{{ customer.first_name | strip }}</span>
    </div>
  </div>
  <div class="flits-mobile-box-card flits-header-title flits-p-10"></div>
  <div class="flits-mobile-container-box flits-mt-25">
    <div class="flits-mobile-box-card flits-p-15"></div>
  </div>
</div>

{%- when 'comp_anchorButton' -%}
<a type="{{ type }}" class="flits-button flits-empty-button {{ class }}" {{ attr }}>{{ text | strip }}</a>

{%- when 'comp_mobile_logutButton' -%}
<a href="/account/logout" class="flits-logout-button">
  <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 11.25 11.24"><path id="Layer_2" data-name="Layer 2" d="M9.19,2.08a5,5,0,1,1-7.07,0L2.19,2M5.59.62V5.51" transform="translate(-0.02 0)" style="fill:none;stroke-linecap:round;stroke-miterlimit:10;stroke-width:1.25px"/></svg>
</a>

{%- when 'comp_emptySection' -%}
<div class="flits-empty-container {{ class }}">
  <div class="flits-empty-icon-box">
    {{ img }}
  </div>
  <div class="flits-empty-text-div">
    <p class="flits-empty-text flits-mb-20">{{ text_1 | strip }}</p>
    <p class="flits-empty-text flits-strong">{{ text_2 | strip }}</p>
  </div>
  {% render 'flits_capture_code' with 'component',template:'comp_anchorButton', text:'View Products',
  attr:'data-flits-lang="buttons.shop_now" data-flits-lang-default="View Products" href="/collections/all"',
  class:'flits-primary-btn flits-mt-20' %}
</div>

{%- when 'comp_noResultSection' -%}
<div class="flits-no-result-container {{ class }}">
  <div class="flits-no-result-icon-box">
    {{ img }}
  </div>
  <p class="flits-no-result-found-text" data-flits-lang="top_ordered_products_page.search_product_not_found" data-flits-lang-default="No product/s found">No product/s found</p>
</div>

{%- when 'comp_loadingSection' -%}
<div class="flits-loading-div {{ class }}" {{ attr }}>
  <div class="flits-spinner"></div>
  <p class="flits-loading-text">{{ text }}</p>
</div>

{%- when 'comp_button' -%}
<button type="{{ type }}" class="flits-button {{ class }}" {{ attr }}>
  {{ text | strip }}
</button>

{%- when 'comp_iconButton' -%}
<button type="{{ type }}" class="flits-button flits-button-icon-wrap {{ class }}" {{ attr }}>
  <div class="flits-button-icon">
    {{ img }}
  </div>
  {{ text | strip }}
</button>

{%- when 'comp_flitsSelect' -%}
<div class="flits-select-row">
  <select class="flits-input {{ class }}"  {{ attr }} name="{{ name | strip }}">
    {{ options }}
  </select>
  <div class="flits-select-arrow">
    <svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
      <path d="M6 9l6 6 6-6"></path>
    </svg> 
  </div>
</div>

{%- when 'comp_flitsLabelSelect' -%}
<div class="flits-input-wrap">
  <label class="flits-input-label">{{ label | strip }}</label>
  <div class="flits-select-row">
    <select class="flits-input {{ class }}"  {{ attr }} name="{{ name | strip }}" >
      {{ options }}
    </select>
    <div class="flits-select-arrow">
      <svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
        <path d="M6 9l6 6 6-6"></path>
      </svg> 
    </div>
  </div>
</div>

{%- when 'comp_flitsLabelInput' -%}
<div class="flits-input-wrap">
  <label class="flits-input-label">{{ label | strip }}</label>
  <input type="{{ type | strip }}" class="flits-input {{ class }}" placeholder="{{ placeholder | strip }}" name="{{ name | strip }}" value="{{ value | strip }}" {{ attr }}>
</div>

{%- when 'comp_flitsInput' -%}
<div class="flits-input-wrap">
  <input type="{{ type | strip }}" class="flits-input {{ class }}" placeholder="{{ placeholder | strip }}" name="{{ name | strip }}" value="{{ value | strip }}" {{ attr }}>
</div>

{%- when 'comp_flitsLabelIconInput' -%}
<div class="flits-input-wrap flits-input-icon-wrap">
  <label class="flits-input-label">{{ label | strip }}</label>
  <div class="flits-input-icon-group">
    <div class="flits-input-icon">
      {{ inputIcon }}
    </div>
    <input type="{{ type | strip }}" class="flits-input {{ class }}" placeholder="{{ placeholder | strip }}" name="{{ name | strip }}" value="{{ value | strip }}" {{ attr }}>
  </div>
</div>

{%- when 'comp_flitsIconInput' -%}
<div class="flits-input-wrap flits-input-icon-wrap">
  <div class="flits-input-icon-group">
    <div class="flits-input-icon">
      {{ inputIcon }}
    </div>
    <input type="{{ type | strip }}" class="flits-input {{ class }}" placeholder="{{ placeholder | strip }}" name="{{ name | strip }}" value="{{ value | strip }}" {{ attr }}>
  </div>
</div>

{%- when 'comp_flitsCheckbox' -%}
<div class="flits-input-row">
  <input type="checkbox" class="flits-input {{ class }}" name="{{ name | strip }}" value="{{ value }}" {{ attr }}>
  <label class="flits-input-label" for="{{ for }}">{{ label | strip }}</label>
</div>

{%- when 'comp_flitsRadio' -%}
<div class="flits-input-row">
  <input type="radio" class="flits-input {{ class }}" name="{{ name | strip }}" value="{{ value }}" {{ attr }}>
  <label class="flits-input-label" for="{{ for }}">{{ label | strip }}</label>
</div>

{%- when 'comp_clipboardInput' -%}
<div class="flits-input-wrap {{ class }}">
  <div class="flits-input-icon-group">
    <input type="{{ type | strip }}" class="flits-input" placeholder="{{ placeholder | strip }}" name="{{ name | strip }}" value="{{ value | strip }}" {{ attr }}>
    {% render 'flits_capture_code' with 'component',template:'comp_button',
    type:'button',
    text:'Copy',
    attr:'data-flits-lang="refer_friend_page.copy" data-flits-lang-default="Copy" id="flits-refer-link-copy"',
    class:'flits-copy-btn flits-primary-btn flits-ml-0'
    %}
  </div>
</div>

{%- when 'comp_productQtyInput' -%}
<div class="flits-product-quantity {{ class }}">
  <input type="{{ type | strip }}" class="flits-input" placeholder="{{ placeholder | strip }}" name="product_quantity_input" value="{{ value | strip }}" {{ attr }} min="{{ min }}">
  <span class="flits-quantity-btn" data-flits-qty-btn="minus">
    <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 11.74 11.74"><path id="Layer_2" data-name="Layer 2" d="M5.87,11.74a5.87,5.87,0,1,1,5.87-5.87A5.87,5.87,0,0,1,5.87,11.74Zm0-11A5.14,5.14,0,1,0,11,5.89s0,0,0,0A5.15,5.15,0,0,0,5.87.73Zm2.57,5.5H3.3a.37.37,0,0,1,0-.74H8.44a.37.37,0,0,1,0,.74Z" transform="translate(0 0)" style="fill:#25d872"/></svg>
  </span>
  <span class="flits-quantity-btn" data-flits-qty-btn="plus">
    <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 11.74 11.74"><path id="Layer_2" data-name="Layer 2" d="M5.87,11.74a5.87,5.87,0,1,1,5.87-5.87A5.87,5.87,0,0,1,5.87,11.74Zm0-11A5.14,5.14,0,1,0,11,5.89s0,0,0,0A5.15,5.15,0,0,0,5.87.73Zm2.57,5.5H3.3a.37.37,0,0,1,0-.74H8.44a.37.37,0,0,1,0,.74ZM5.87,8.8a.37.37,0,0,1-.37-.36V3.3a.37.37,0,1,1,.74,0h0V8.44A.37.37,0,0,1,5.87,8.8Z" transform="translate(0 0)" style="fill:#25d872"/></svg>
  </span>
</div>

{%- when 'comp_flitsPagination' -%}
<div class="flits-pagination-div {{ class }}" {{ attr }}>
  {% comment %} <button class="flits-pagination-btn flits-first-btn" data-flits-btn="flits-prev-btn" data-flits-lang="buttons.first" data-flits-lang-default="First">First</button> {% endcomment %}
  <button class="flits-pagination-btn flits-prev-btn" data-flits-btn="flits-prev-btn" data-flits-lang="buttons.previous" data-flits-lang-default="Previous">Previous</button>
  <ul class="flits-pagination pagination"></ul>
  <button class="flits-pagination-btn flits-next-btn" data-flits-btn="flits-next-btn" data-flits-lang="buttons.next" data-flits-lang-default="Next">Next</button>
  {% comment %} <button class="flits-pagination-btn flits-last-btn" data-flits-btn="flits-next-btn" data-flits-lang="buttons.last" data-flits-lang-default="Last">Last</button> {% endcomment %}
</div>
<div class="flits-clearfix"></div>

{%- when 'comp_flitsSocialLoginLoader' -%}
{% render 'flits_capture_code' with 'component',template:'comp_loadingSection', class:'flits-social-login-loader', attr:'style=display:none' text:socialLogin_loadingLabelText %}

{%- when 'comp_contactUsForm' -%}
<script>
  if (!window.flitsApp || typeof window.flitsApp == "undefined") {
    window.flitsApp = {};
  }
</script>
<style>
  .flits-overlay.flits-order-contact-us-popup {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.7);
    transition: opacity 0.5s;
    visibility: hidden;
    opacity: 0;
    z-index: 1111;
    font-size: 15px;
    font-family: inherit;
    font-style: normal;
    font-weight: 400;
    color: #3a3a3a;
    line-height: 1.5;
  }
</style>
<div id="flits-contact-us-form" class="flits-overlay flits-order-contact-us-popup" style="visibility:hidden">
  <div class="flits-popup">
    <div class="flits-popup-icon-text">
      <div class="flits-popup-icon">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svgjs="http://svgjs.com/svgjs" version="1.1" width="512" height="512" x="0" y="0" viewBox="0 0 420.96 420.96" style="enable-background:new 0 0 512 512" xml:space="preserve" class="">
          <g>          
            <g xmlns="http://www.w3.org/2000/svg">
              <g>
                <path d="M354.48,144c0-79.529-64.471-144-144-144s-144,64.471-144,144c-20.56,2.56-40,16-40,47.2v75.2c0,35.2,24.64,48,48,48h23.2    c4.418,0,8-3.582,8-8V151.52c0-4.418-3.582-8-8-8h-15.2c0-70.692,57.308-128,128-128s128,57.308,128,128h-15.2    c-4.418,0-8,3.582-8,8v154.64c0,4.418,3.582,8,8,8h15.2c-2.24,54.96-28.64,67.84-81.36,70.16v-5.36c0-8.837-7.163-16-16-16H190.8    c-8.837,0-16,7.163-16,16v26c0,8.837,7.163,16,16,16h50.48c8.837,0,16-7.163,16-16v-4.64c50.72-2.32,94.56-12.8,97.2-86.64    c20.56-2.56,40-16,40-47.2v-75.2C394.48,159.92,375.12,146.56,354.48,144z M89.68,158.96v139.2h-15.6c-11.84,0-32-4.16-32-32    v-75.2c0-27.6,19.92-32,32-32H89.68z M241.28,392.88v12.16H190.8v-26h50.48V392.88z M378.48,266.4c0,27.6-19.92,32-32,32h-15.2    V159.52h15.6c11.84,0,32,4.16,32,32L378.48,266.4z" fill="#ffffff" data-original="#000000" style="" class=""/>
