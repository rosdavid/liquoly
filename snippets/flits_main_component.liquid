{%- when 'comp_topOrderProductCard'-%}
<div class="flits-top-order-card {{ class }} flits-product-card">
  <div class="flits-order-item">
    <a><p class="flits-product-name flits-link flits-text-ellipsis"></p></a>
    <div class="flits-product-image-thumbnail">
      <img src="">
    </div>
    <p class="flits-product-price"></p>
  </div>
  <div class="flits-order-details">
    <div class="flits-product-quantity-header">
      <span data-flits-lang="top_ordered_products_page.number_of_order" data-flits-lang-default="Total quantity ordered till now">Total quantity ordered till now</span>:&nbsp<span class="flits-product-quantity-count"></span>
    </div>
    <div class="flits-product-action-group">
      {% render 'flits_capture_code' with 'component',template:'comp_flitsSelect',class:'flits-variant-select' %}
